{
  "manifest_version": 3,
  "name": "LeetCode Question Extractor",
  "version": "1.0",
  "description": "Extracts question content from LeetCode",
  "permissions": [
    "activeTab",
    "scripting",
    "webNavigation"
  ],

  "content_scripts" : [
    {"matches" : ["<all_urls>"],
    "js" : ["content.js"]
  }
  ],

  "action": {
    "default_popup": "popup.html"
  },

  "web_accessible_resources": [
        {
            "resources": ["question.txt"],
            "matches": ["<all_urls>"]
        }
    ]
}